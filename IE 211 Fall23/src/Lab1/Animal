class Animal:

    __me = {
        "name" : None,
        "id" : 0
    }

    def __init__(self):
        pass

    def setName(self, name):
        self.__me["name"] = name

    def setID(self, id):
        self.__me["id"] = id

    def __getName(self):
        return self.__me["name"]

    def getID(self):
        return self.__me["id"]

    def speak(self):
        print("Hi all :)")
