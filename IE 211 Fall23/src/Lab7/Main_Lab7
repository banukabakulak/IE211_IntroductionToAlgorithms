from Coverage import Coverage

U = [1, 2, 3, 4, 5]
f = [1, 1, 1, 1, 1]

sets = []
set1 = [1, 3, 4]
sets.append(set1)
set2 = [2, 5]
sets.append(set2)
set3 = [1, 2, 3, 4]
sets.append(set3)

p = [5, 10, 3]

print("The sets are ")
for i in range(len(sets)):
    print()
    for j in range(len(sets[i])):
        print(sets[i][j], end=" ")

set_coverage = Coverage()
set_coverage.ImplementAlgorithm(f, sets, p)
