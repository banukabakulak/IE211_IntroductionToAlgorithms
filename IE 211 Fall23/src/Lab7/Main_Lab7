from Coverage import Coverage

print("IE 211 Lab 7 Codes...")

U = [1, 2, 3, 4, 5]
f = [1, 2, 1, 2, 1]

sets = []
set1 = [1,2]
sets.append(set1)
set2 = [2,3,4]
sets.append(set2)
set3 = [2,5]
sets.append(set3)
set4 = [3, 4]
sets.append(set4)
set5 = [1,4]
sets.append(set5)

p = [5, 6, 10, 2, 4]

print("The subsets are ")
print(*sets)

setCoverage = Coverage()
setCoverage.ImplementAlgorithm(f, sets, p)

